    "sex",
    "equal",
    "equality",
    "race",
    "religion",
    "orientation",
    "consideration",
    "natural-language",
    "english",
    "wrapper",
    "bridge",
    "lint",
    "linter",
    "check",
    "checker",
    "validate",
    "validation",
    "vsc",
    "vscode",
    "visual-studio-code",
    "diagnostic",
    "diagnostics"
  ],
  "dependencies": {
    "alex": "^1.6.1",
    "is-md": "^1.0.0",
    "vfile-messages-to-vscode-diagnostics": "^1.0.0"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node": "^1.0.1",
    "eslint": "^1.10.3",
